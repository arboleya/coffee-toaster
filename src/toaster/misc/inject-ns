#<< toaster/toast

fs = require 'fs'
path = require 'path'
exec = (require "child_process").exec

class InjectNS

	constructor:( @builders )->

		console.log "Declaring namespaces for files..."
		for builder in @builders
			for f in builder.files
				f.getinfo true
				fs.writeFileSync f.realpath, f.raw
				console.log f.realpath
		console.log "...done."